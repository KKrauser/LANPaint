<Application x:Class="LANPaint.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:view="clr-namespace:LANPaint.Views"
             xmlns:viewModels="clr-namespace:LANPaint.ViewModels"
             Startup="OnStartup"
             StartupUri="Views/Paint.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type viewModels:SettingsViewModel}">
            <view:Settings/>
        </DataTemplate>
    </Application.Resources>
</Application>
