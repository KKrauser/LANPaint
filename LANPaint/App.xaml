<Application x:Class="LANPaint.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:alert="clr-namespace:LANPaint.Dialogs.Alerts"
             xmlns:view="clr-namespace:LANPaint.Views"
             xmlns:viewModels="clr-namespace:LANPaint.ViewModels"
             StartupUri="Views/Paint.xaml">
    <Application.Resources>
         <DataTemplate DataType="{x:Type alert:AlertDialogViewModel}">
             <alert:Alert/>
         </DataTemplate>
        <DataTemplate DataType="{x:Type viewModels:SettingsViewModel}">
            <view:Settings/>
        </DataTemplate>
    </Application.Resources>
</Application>
